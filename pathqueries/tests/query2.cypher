MATCH (gene:Gene)-[]-(gene_homologue:Homologue), (gene_homologue)-[]-(gene_homologue_dataset:DataSet), (gene_homologue_dataset)-[]-(gene_homologue_dataset_publication:Publication), (gene_homologue)-[]-(gene_homologue_evidence:Evidence), (gene_homologue_evidence)-[]-(gene_homologue_evidence_publication:Publication) WHERE gene_homologue_evidence_publication.abstractText CONTAINS 'a' AND gene_homologue_dataset.name CONTAINS 'a' RETURN gene.length, gene_homologue_evidence_publication.title